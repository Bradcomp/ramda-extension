{
  "name": "ramda-extension-build",
  "private": true,
  "version": "0.0.1",
  "description": "Utilities function built on top of Ramda",
  "main": "./src/index.js",
  "scripts": {
    "build": "npm run build:js && npm run copy",
    "build:js": "cross-env NODE_ENV=release node_modules/.bin/babel ./src --out-dir ./build --ignore __tests__/*",
    "prebuild": "npm run clean",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "clean": "rimraf build",
    "copy": "babel-node scripts/run copy",
    "test": "npm run lint && npm run test:js",
    "test:js": "cross-env NODE_ENV=test BABEL_JEST_STAGE=0 jest --config=./jest.config.json",
    "test:watch": "npm run test:js -- --watch",
    "lint:eslint": "eslint --ext .js ./"
  },
  "author": "",
  "license": "proprietary",
  "peerDependencies": {
    "ramda": "^0.22.0"
  },
  "devDependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-jest": "^14.1.0",
    "babel-plugin-transform-class-properties": "6.11.5",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "0.2.9",
    "babel-plugin-transform-regenerator": "6.14.0",
    "babel-plugin-transform-runtime": "6.12.0",
    "babel-preset-latest": "6.14.0",
    "babel-preset-stage-1": "6.13.0",
    "babel-runtime": "6.11.6",
    "cross-env": "2.0.0",
    "es6-promisify": "4.1.0",
    "eslint": "3.3.1",
    "eslint-config-react": "git://github.com/tommmyy/eslint-config-react.git",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-react": "6.1.1",
    "fs-extra": "^0.30.0",
    "jest": "15.1.1",
    "ramda": "^0.22.0",
    "rimraf": "2.5.3"
  }
}
