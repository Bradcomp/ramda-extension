{
  "name": "ramda-extension",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/tommmyy/ramda-extension.git"
  },
  "keywords": [
    "ramda",
    "ramdajs",
    "utils",
    "functional programming",
    "case conversion",
    "PascalCase",
    "dot.case",
    "camelCase",
    "lower first letter",
    "upper first letter"
  ],
  "description": "Utilities functions built on top of Ramda",
  "module": "es/index.js",
  "main": ".lib/index.js",
  "files": [
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir lib",
    "build:umd": "cross-env BABEL_ENV=rollup NODE_ENV=development rollup -c -o dist/ramda-extension.js",
    "build:umd:min": "cross-env BABEL_ENV=rollup NODE_ENV=production rollup -c -o dist/ramda-extension.min.js",
    "build": "npm run build:es && npm run build:cjs && npm run build:umd && npm run build:umd:min",
    "lint": "npm run lint:eslint",
    "clean": "rimraf es lib dist",
    "test": "npm run lint && npm run test:js",
    "test:js": "jest --config=./jest.config.json",
    "test:watch": "npm run test:js -- --watch",
    "lint:eslint": "eslint --ext .js ./"
  },
  "author": "Tomas Konrady <tomaskonrady@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "ramda": "^0.24.0 || ^0.25.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "6.1.2",
    "babel-jest": "21.2.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-1": "^6.24.1",
    "cross-env": "2.0.0",
    "eslint": "3.3.1",
    "eslint-config-react": "git://github.com/tommmyy/eslint-config-react.git",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-react": "6.1.1",
    "fs-extra": "^0.30.0",
    "jest": "21.2.1",
    "ramda": "^0.25.0",
    "rimraf": "2.5.3",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1"
  }
}
